{
  "name": "manageros-starter",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint-staged": "lint-staged",
    "prisma:generate": "bunx prisma generate",
    "prisma:push": "bunx prisma db push",
    "prisma:seed": "bun prisma/seed.ts",
    "db:init": "bun scripts/init-database.ts",
    "db:backup": "bun scripts/backup-database.ts --verbose",
    "db:restore": "bun scripts/restore-database.ts",
    "help:generate": "bun scripts/generate-help-content.ts",
    "help:list": "bun scripts/help-content-manager.ts list",
    "help:validate": "bun scripts/help-content-manager.ts validate",
    "help:index": "bun scripts/help-content-manager.ts index",
    "help:clear": "bun scripts/help-content-manager.ts clear",
    "cleanup:tokens": "bun scripts/cleanup-expired-tokens.ts",
    "test:email": "bun scripts/test-email-config.ts",
    "cron:run": "bun scripts/run-cron-jobs.ts",
    "cron:birthdays": "bun scripts/run-cron-jobs.ts --job birthday-notification",
    "cron:activity": "bun scripts/run-cron-jobs.ts --job activity-monitoring",
    "version:patch": "bun scripts/bump-version.ts patch",
    "version:minor": "bun scripts/bump-version.ts minor",
    "version:major": "bun scripts/bump-version.ts major",
    "postinstall": "bunx prisma generate && bun run help:generate",
    "prepare": "husky",
    "release": "release-it"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.44",
    "@ai-sdk/react": "^2.0.60",
    "@aws-sdk/client-s3": "^3.895.0",
    "@aws-sdk/s3-request-presigner": "^3.895.0",
